{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "BooBoo",
  "version": "0.1.0",
  "identifier": "com.anchorwon.booboo",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Booboo OCR",
        "width": 450,
        "height": 550,
        "transparent": true,
        "decorations": false,
        "visible": false,
        "skipTaskbar": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}